<h3>Student Form</h3>

<form method="POST" action="<%=students_path%>">
  <label>First Name:</label>
  <input type="text" name="students[first_name]" id="first_name">

  <label>Last Name:</label>
  <input type="text" name="students[last_name]" id="last_name">

  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <input type="submit" value="Submit Student">
</form>
